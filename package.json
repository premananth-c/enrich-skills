{
  "name": "enrich-skills",
  "version": "0.1.0",
  "private": true,
  "description": "White-label SaaS Mock Test & Coding Assessment Platform",
  "scripts": {
    "dev": "pnpm --filter @enrich-skills/shared build && pnpm -r --parallel run dev",
    "dev:api": "pnpm --filter @enrich-skills/api dev",
    "dev:student": "pnpm --filter @enrich-skills/student-web dev",
    "dev:admin": "pnpm --filter @enrich-skills/admin-web dev",
    "build": "pnpm -r run build",
    "db:generate": "pnpm --filter @enrich-skills/api exec prisma generate",
    "db:push": "pnpm --filter @enrich-skills/api exec prisma db push",
    "db:migrate": "pnpm --filter @enrich-skills/api exec prisma migrate dev",
    "db:seed": "pnpm --filter @enrich-skills/api exec tsx prisma/seed.ts"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "packageManager": "pnpm@9.0.0"
}
